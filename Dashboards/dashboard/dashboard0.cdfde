ercent\r\n    };\r\n    */\r\n    cde.valuesOverflow = 'Show';\r\n    cde.valuesVisible = true;\r\n    \r\n    // Chart/Interaction\r\n    cde.animate = true;\r\n    cde.selectable = true;\r\n    cde.hoverable = true;\r\n    \r\n    //tooltips\r\n    cde.tooltipDelayIn = 20;\r\n    cde.tooltipDelayOut = 20;\r\n    cde.tooltipFade = true;\r\n    cde.tooltipGravity = 's';\r\n    cde.tooltipOpacity = 1;\r\n    cde.tooltipFollowMouse = true;\r\n    cde.tooltipEnabled = true; //提示框是否显示\r\n    //提示框格式设置\r\n    cde.tooltipFormat = function f(){\r\n        var series = this.getSeriesLabel(),\r\n            category = this.getCategory(),\r\n            value = myPie2.addCommas(this.getValue().toFixed(2));\r\n        return \"<div class='tooltipContainer'>\" +\r\n            \"<div class='tooltipTitle'>\" + category + \"</div>\" +  \r\n            //\"<div class='tooltipLabel'>\" + series + \"</div>\" +  \r\n            \"<div class='tooltipValue'>￥\" + value + \"</div>\" + \r\n        \"</div>\";\r\n    };\r\n    \r\n} "
