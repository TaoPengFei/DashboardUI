opensans-regular.eot') format('embedded-opentype'),\n        url('../../../../../system/common-ui/resources/themes/crystal/opensans-regular.woff') format('woff'),\n        url('../../../../../system/common-ui/resources/themes/crystal/opensans-regular.ttf') format('truetype'),\n        url('../../../../../system/common-ui/resources/themes/crystal/opensans-regular.svg#OpenSansRegular') format('svg');\n}\n\n/* ================ BUTTON RANGE ================ */\n.filterRow{\n    height:20px;\n\t/*border:1px solid #c2c2c2;\n\tmargin:5px 5px 20px;\n    */\n}\n.filterWrapper{\n    margin:0 auto;\n\tdisplay: inline-block;\n    /*\n\tmargin:10px;\n    */\n}\n\n.filter,.filterLabel{\n\tdisplay: inline-block;\n}\n.filterLabel{\n    font-size:15px;\n    width:50px;\n    text-align:right;\n    \n}\n#buttonfilter{\n    /*\n    #F4F4F4  #368A1D\n    */\n    background-color: #F4F4F4;\n    color: black;\n}\n#yearFilter select,#monthFilter select,#dayFilter select{\n    border-radius: 0px;\n}\n\n\n/* ================ CDE CHARTS BEFORE RANGE ================ */\n.chartHeader{\n    text-align: center;\n    font-size: 20px;\n    font-family: 'Roboto',sans-serif;\n    padding: 5px;\n    margin: 5px 10px;\n}\n.pieChart1,.pieChart2,.pieChart3{\n\theight:350px;\n\t/*border:1px solid #c2c2c2;*/\n\tmargin:5px;\n}\n.barChart{\n\t/*border:1px solid #c2c2c2;*/\n\theight:710px;\n\tmargin:5px;\n} \n\n/* ================ YEAR RANGE ================ */\n\n.yearObject {\n    margin-top: 10px;\n    /*\n    margin-right: 45%;\n    */\n    margin-left: 20px;\n}\n\n.yearObject .chzn-container {\n    width: 100px !important;\n    font-size: 16px;\n    font-weight: bold;\n    display: block;\n    float: right;\n}\n\n/* ================ MONTH RANGE ================ */\n\n.monthObject {\n    margin-top: 10px;\n    /*\n    margin-right: 45%;\n    */\n    margin-left: 20px;\n}\n\n.monthObject .chzn-container {\n    width: 100px !important;\n    font-size: 16px;\n    font-weight: bold;\n    display: block;\n    float: right;\n}\n\n/* ================ DAY RANGE ================ */\n\n.dayObject {\n    margin-top: 10px;\n    /*\n    margin-right: 45%;\n    */\n    margin-left: 20px;\n}\n\n.dayObject .chzn-container {\n    width: 100px !important;\n    font-size: 16px;\n    font-weight: bold;\n    display: block;\n    float: right;\n}\n\n.refreshBtnObj{\n    margin-top : 5px;\n}\n\n\n/* ================ 饼状图css设计 ================ */\n\n.tipsy-inner {\n    background: #6F6F6F;\n    font-family: 'Open Sans', Arial, sans-serif;\n    border: none;\n    /*\n    -webkit-box-shadow: 1px 1px 2px rgba(0,0,0,.2);      设定元素的边框为1像素宽，实线，颜色 \n      -msie-box-shadow: 1px 1px 2px rgba(0,0,0,.2);\n       -moz-box-shadow: 1px 1px 2px rgba(0,0,0,.2);\n         -o-box-shadow: 1px 1px 2px rgba(0,0,0,.2);\n            box-shadow: 1px 1px 2px rgba(0,0,0,.2);\n            */\n    padding: 8px 10px 9px;      /*提示框长宽高设置*/\n    /*color: #4d4d4d;*/\n    color: #6F6F6F;\n    max-width: none;\n    border: 1px solid #dedede;\n}\n\n.tipsy-s .tipsy-arrow {\n    display: none !important;\n}\n\n.tooltipContainer .tooltipTitle {\n    color: #FFFFFF;\n    display: block;\n    text-align: center;\n    font-size: 14px;\n    margin-bottom: 10px;\n    line-height: 1;\n}\n\n.tooltipContainer .tooltipLabel {\n    color: #4d4d4d;\n    display: block;\n    text-align: center;\n    font-size: 12px;\n    margin-bottom: 10px;\n    line-height: 1;\n}\n\n.tooltipContainer .tooltipValue {\n    display: block;\n    text-align: center;\n    font-size: 24px;\n    font-weight: 600;\n    line-height: 1;\n    color: #FFFFFF;\n}\n\n.tooltipContainer.target .tooltipValue {\n    color: #f46d43;\n}\n.tooltipContainer.sales .tooltipValue {\n    color: #01665e;\n} \n\n\n/* ================  表格的css设计 ================ */\n#div3Table:hover{\n    cursor: pointer;\n}\n/*\n\n\n\n\n\n\n "
