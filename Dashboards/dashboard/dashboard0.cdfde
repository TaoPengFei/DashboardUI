     {
      "name": "postExecution",
      "type": "JavaScript",
      "value": "function f(){\n    /**\n     *  金额 类的 千分位 格式化\n     */\n    var format_guestPrice = function(nStr){\n        nStr += '';\n        x = nStr.split('.');\n        x1 = x[0];\n        x2 = x.length > 1 ? '.' + x[1] : '';\n        var rgx = /(\\d+)(\\d{3})/;\n        while (rgx.test(x1)){\n            x1 = x1.replace(rgx, '$1' + ',' + '$2');\n        }\n        x2 = x2.slice(0,3);\n        return x1 + x2;\n    };\n    /**\n     *  金额 类的  为0 的 格式化\n     */\n    var change = function(num){\n        num = parseFloat(num);\n        if( isNaN(num) ){\n            return false;\n        }else{\n            var newNum = \"\";\n            num = num + \"\";\n            var temp = num.split(\"\\.\");\n            var myLength = 0;\n            if( 2 <= temp.length ){\n                myLength = temp[1].length;\n            }\n            switch( myLength ){\n                case 0 :\n                    newNum = num + \".00\";\n                    break;\n                case 1 :\n                    newNum = num + \"0\";\n                    break;\n                default :\n                    newNum = num;\n                     break;\n            }\n            return newNum;\n        }\n    };\n    if( my_guestPrice == \"\" ){\n        my_guestPrice[0][0] = 0;\n        //document.getElementById('guestPriceHtmlObj').innerHTML = change(my_guestPrice[0][0]);\n    }else{\n        document.getElementById('guestPriceHtmlObj').innerHTML = format_guestPrice(my_guestPrice[0][0]);\n    }\n    //document.getElementById('guestPriceHtmlObj').innerHTML = format_guestPrice(my_guestPrice[0][0]);   \n    console.log(\"my_guestPrice = \" + my_guestPrice);\n    console.log(\"my_guestPrice[0][0] = \" + my_guestPrice[0][0]);\n    console.log(\"format_guestPrice(my_guestPrice[0][0]) = \" + format_guestPrice(my_guestPrice[0][0]));\n} "
     },
