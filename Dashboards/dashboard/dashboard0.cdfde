     {
      "name": "resourceCode",
      "type": "Resource",
      "value": "/**设置第一个图形的提示框显示样式*/\r\n/**鼠标停留在图形上面的显示样式*/\r\n/**\r\n * 创建一个全局变量myPie,用于定制自己的柱状图。\r\n*/\r\nvar myPie1= {}; \r\n/**\r\n *  格式一：数字添加千分位符函数\r\n*/\r\n/*\r\nmyPie1.addCommas = function (nStr){\r\n    nStr += '';\r\n    x = nStr.split('.');\r\n    x1 = x[0];\r\n    x2 = x.length > 1 ? '.' + x[1] : '';\r\n    var rgx = /(\\d+)(\\d{3})/;\r\n    while (rgx.test(x1)){\r\n        x1 = x1.replace(rgx, '$1' + ',' + '$2');\r\n    }\r\n    x2 = x2.slice(0,3);\r\n    return x1 + x2;\r\n}; \r\n*/\r\n/**\r\n *  格式二：数字添加千分位符函数\r\n*/\r\nmyPie1.addCommas =  function (num){\r\n    // 判断 是否 是 数字\r\n    /*\r\n    if(typeof num !==\"number\"){\r\n        return false;\r\n    }\r\n    */\r\n    num = num.toString();   // 字符 化\r\n    var s = num.match(/e\\+(\\d+)$/),ext=0;;\r\n    num = num.replace(/e.+$/,'');\r\n    if( s ){\r\n        ext=Number(s[1]);\r\n    }\r\n    //分割小数点两边\r\n    var tA=num.split('.');\r\n    //有小数点则分割开来处理(小数点后面可能还跟有科学记数法表示)\r\n    if( tA.length>=2&&ext ){\r\n        if( tA.length>ext ){\r\n            tA[0] += tA[1].slice(0,ext-1);\r\n            tA[1] = tA[1].slice(ext-1,tA[1].length-1);\r\n        }else{\r\n            tA[0] += (tA[1]+\"0\".repeat(ext-tA.length));\r\n            tA[1]='';\r\n        }\r\n    }\r\n    tA[0] = tA[0].split('');//拆字符\r\n    for( var i=tA[0].length;(i-=3)>0; ){\r\n        tA[0].splice(i,0,',');\r\n    }\r\n    return tA[0].join('')+(tA[1]?'.'+tA[1]:'');//连起来\r\n}\r\n/**\r\n * 定制自己的饼图\r\n*/\r\nmyPie1.defaultPieSettings = function (){\r\n    var cde = this.chartDefinition;\r\n    \r\n    //general\r\n    //cde.width = 550;\r\n    cde.height = 280;\r\n    \r\n    // Data source\r\n    cde.crosstabMode = true;\r\n    \r\n    // Panels\r\n    //cde.title = \" 品牌可点击\",\r\n    //cde.titleSize = {width: '100%'},\r\n    cde.titlePaddings = '4%',\r\n    cde.titleFont = 'lighter 20px \"Open Sans\"',\r\n    cde.title_fillStyle = '#FFFFFF',\r\n    cde.titleLabel_textStyle = '#333333',\r\n    \r\n    \r\n    // Main Plot\r\n    cde.valuesOptimizeLegibility = true;\r\n    cde.valueVisible = true;\r\n    cde.valueFont = 'lighter 5px \"Open Sans\"';\r\n    cde.explodedSliceIndex = 1;\r\n    cde.explodedSliceRadius = '2%'; //饼图部分分离\r\n    cde.slice_strokeStyle = 'white';\r\n    cde.slice_offsetRadius = function(scene) {\r\n        return scene.isSelected() ? '5%' : '5%';\r\n        //alert(scene.isSelected());\r\n    },\r\n    \r\n    //// Color axes\r\n    cde.colors = [\r\n         /*\r\n        '#F4A20E', '#8EC21F', '#E70012'\r\n        */\r\n        '#F39C12','#00A65A','#DD4B39'\r\n    ];\r\n    \r\n    //Panels\r\n    cde.legend = true;\r\n    cde.legendShape = 'circle';\r\n    //cde.legendSize = {\r\n     //   width : '100%'\r\n    //};\r\n    cde.legendArea_fillStyle = '#FFFFFF';\r\n    cde.legendFont = 'normal 12px \"Open Sans\"';\r\n    cde.valuesFont = \"lighter 12px Open Sans\";\r\n    cde.valuesAnchor = 'center';\r\n    //cde.valuesLabelStyle = 'inside';\r\n    cde.valuesLabelStyle = 'linker';\r\n    /*\r\n    cde.valuesMask = {\r\n        value.percent\r\n        {category}={value}\\x0a({value.percent})\r\n    };\r\n    */\r\n    cde.valuesOverflow = 'Show';\r\n    cde.valuesVisible = true;\r\n    \r\n    // Chart/Interaction\r\n    cde.animate = true;\r\n    cde.selectable = true;\r\n    //cde.hoverable = true;\r\n    \r\n    //tooltips\r\n    cde.tooltipDelayIn = 20;\r\n    cde.tooltipDelayOut = 20;\r\n    cde.tooltipFade = true;    // 注意 设为true 的 情况\r\n    cde.tooltipGravity = 's';\r\n    cde.tooltipOpacity = 1;\r\n    cde.tooltipFollowMouse = true;\r\n    cde.tooltipEnabled = true; //提示框是否显示\r\n    //提示框格式设置\r\n    cde.tooltipFormat = function f(){\r\n        var series = this.getSeriesLabel(),\r\n            category = this.getCategory(),\r\n            value = myPie1.addCommas(this.getValue().toFixed(2));\r\n            //value = this.getValue();\r\n        return \"<div class='tooltipContainer'>\" +\r\n            \"<div class='tooltipTitle'>\" + category + \"</div>\" +  \r\n            //\"<div class='tooltipLabel'>\" + series + \"</div>\" +  \r\n            \"<div class='tooltipValue'>￥\" + value + \"</div>\" +\r\n        \"</div>\";\r\n    };\r\n}\r\n "
     },
