og(\"OutLetId = \" + OutLetId);\r\n        \r\n// Now, remove the selected mark from other rows and mark this\r\n        row.parent().find(\"tr\").removeClass(\"selected\");\r\n        row.addClass(\"selected\");\r\n    });\r\n    \r\n    // Remove selected class from all rows and set it on the clicked row  numeric\r\n    tableObj.removeClass('selected');\r\n    tableObj.each(function(idx, elem){\r\n        var outlet = $(elem).find('.numeric').text();\r\n        console.log(\"here = \" + outlet);\r\n        if ( outlet == OutLetId ){\r\n            $(elem).addClass('selected');\r\n        }\r\n    });\r\n} ","description":"Post Execution","tooltip":"Function to be executed after the component is updated","order":93,"classType":"advanced"},{"name":"postFetch","type":"JavaScript","value":"","description":"Post Fetch","tooltip":"Code executed after this component's data is fetched from the server","order":93,"classType":"advanced"},{"name":"preChange","type":"JavaScript","value":"","description":"Pre Change","tooltip":"Function to be executed before the component is changed. It's a function of the type function(value){return value}, and can be used for validation","order":94,"classType":"advanced"},{"name":"postChange","type":"JavaScript","value":"","description":"Post Change","tooltip":"Function to be executed after the component is changed. It's a function of the type function(value){...}","order":95,"classType":"advanced"},{"name":"extraOptions","type":"ExtraOptionsArray","value":"[]","description":"Extra Options","tooltip":"Extra Options","order":99,"classType":"advanced"},{"name":"tooltip","type":"Html","value":"<html>\n    <head>\n        <title>首页仪表盘</title>\n        <!-- Tell the browser to be responsive to screen width -->\n\t    <meta content=\"width=device-width, initial-scale=1, maximum-scale=1, user-scalable=no\" name=\"viewport\">\n        <link rel=\"stylesheet\" href=\"https://cdnjs.cloudflare.com/ajax/libs/font-awesome/4.5.0/css/font-awesome.min.css\"