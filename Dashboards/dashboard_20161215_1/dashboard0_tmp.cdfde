2);\n            */\n    padding: 8px 10px 9px;      /*提示框长宽高设置*/\n    /*color: #4d4d4d;*/\n    color: #6F6F6F;\n    max-width: none;\n    border: 1px solid #dedede;\n}\n\n.tipsy-s .tipsy-arrow {\n    display: none !important;\n}\n\n.tooltipContainer .tooltipTitle {\n    color: #FFFFFF;\n    display: block;\n    text-align: center;\n    font-size: 14px;\n    margin-bottom: 10px;\n    line-height: 1;\n}\n\n.tooltipContainer .tooltipLabel {\n    color: #4d4d4d;\n    display: block;\n    text-align: center;\n    font-size: 12px;\n    margin-bottom: 10px;\n    line-height: 1;\n}\n\n.tooltipContainer .tooltipValue {\n    display: block;\n    text-align: center;\n    font-size: 24px;\n    font-weight: 600;\n    line-height: 1;\n    color: #FFFFFF;\n}\n\n.tooltipContainer.target .tooltipValue {\n    color: #f46d43;\n}\n.tooltipContainer.sales .tooltipValue {\n    color: #01665e;\n} \n\n\n/* ================  表格的css设计 ================ */\n#div3Table:hover{\n    cursor: pointer;\n}\n/*\n\n\n\n\n\n\n ","description":"Resource code","tooltip":"Resource code to include","order":19,"classType":""},{"name":"resourceType","type":"Label","value":"Css","description":"Type","tooltip":"Resource Type to include","order":19,"classType":""}],"type":"LayoutResourceCode","typeDesc":"Resource"},{"id":"57caaeea-d248-4fc1-048d-63387edf61e2","parent":"UnIqEiD","properties":[{"name":"name","type":"Id","value":"eventFunc_js","description":"Name","tooltip":"Name","order":5,"classType":""},{"name":"resourceCode","type":"Resource","value":"/**\n * 鼠标事件\n */\n$(\".filterWrapper\").on(\"mouseenter\",function(){\n    var $this = $(this);\n    $this.find(\"span\").show();\n}).on(\"mouseleave\",function(){\n    var $this = $(this);\n    $this.find(\"span\").hide();\n})\n\n\n/**\n * CDE图形恢复初始化\n */\n/*\nfunction refreshBtn(){\n    Dashboards.fireChange(\"p_color\",\"rgb(30, 44, 148)\");\n    Dashboards.fireChange(\"p_refresh\",1);\n    //$(\"#pieChart3\").css(\"display\",\"none\");\n    //$(\"#pieChart1\").css(\"display\",\"block\");\n    //Dashboards.update(render_query1);\n    Dashboards.fireChange(\"yearParam\",2016);\n    Dashboards.fireChange(\"monthParam\",1);\n    Dashboards.fireChange(\"dayParam\",1);\n    \n    Dashboards.fireChange(\"BrandId\",0);\n    Dashboards.fireChange(\"AreaId\",0);\n    Dashboards.fireChange(\"OutLetId\",0);\n    \n    \n    \n    //Dashboards.fireChange(\"p_year\",p_year);\n    //Dashboards.update(render_p_month);\n    //Dashboards.update(render_query2);\n    //Dashboards.fireChange(\"p_sort\",v_sort);\n}\n*/\n$(function(){\n    $(\"button\").bind(\"click\",function(){\n        Dashboards.fireChange(\"p_color\",\"rgb(30, 44, 148)\");\n        Dashboards.fireChange(\"p_refresh\",1);\n        //$(\"#pieChart3\").css(\"display\",\"none\");\n        //$(\"#pieChart1\").css(\"display\",\"block\");\n        //Dashboards.update(render_query1);\n        Dashboards.fireChange(\"yearParam\",2016);\n        Dashboards.fireChange(\"monthParam\",1);\n        Dashboards.fireChange(\"dayParam\",1);\n        \n        Dashboards.fireChange(\"BrandId\",0);\n        Dashboards.fireChange(\"AreaId\",0);\n        Dashboards.fireChange(\"OutLetId\",0);\n        \n        //Dashboards.fireChange(\"p_year\",p_year);\n        //Dashboards.update(render_p_month);\n        //Dashboards.update(render_query2);\n        //Dashboards.fireChange(\"p_sort\",v_sort);           \n    });\n});\n/*\n$(function () { $(\"input[type=button][name='dd']\").bind(\"click\", function () { alert(\"ddd\"); }); }\n$(document).ready(function(){\n    $(\".last\").parent().parent().removeClass(\"container\").addClass(\"container-fluid\");\n})\n*/\n\n\n\n/**\n * \n */\n\n \n \n/**\n * 解决CDE图形的自适应\n */\n /*\nfunction resizeGraphical(cdeObj){\n    var myself = cdeObj;\n    //设置初始宽度 与placeholder()匹配\n    myself.chartDefinition.width = myself.placeholder().width();\n    //仅在图表组件的第一个执行上附加调整处理程序\n    if( !cdeObj.resizeHandlerAttached ){\n        //确保渲染只在事件发生后被触发\n        var debouncedResize = _.debounce(function(){\n            //再次显示 图形\n            myself.placeholder().children().css('visibility','visible');\n            // 改变 图形 的 宽度\n            myself.chartDefinition.width = myself.placeholder().width();\n            myself.render( myself.query.lastResults() );\n        },200);\n        // 高度调整 处理\n        $(window).resize(function(){\n            //仅当容器已更改宽度时才触发调整大小\n            if( myself.chartDefinition.width != myself.placeholder().width() ){\n                //暂时隐藏图表，使溢出不会发生\n                myself.placeholder().children().css('visibility','hidden');\n                //触发与调整\n                debouncedResize();\n            }\n        });\n        cdeObj.resizeHandlerAttached = true;\n    }\n} \n*/\nfunction func_resize(myobj)/*解决图形的自适应问题*/   /*不过这里没有起到实质的作用*/\n{\n    var myself = myobj;\n  // Set initial width to match the placeholder\n  myself.chartDefinition.width = myself.placeholder().width();\n\n  // Attach the resize handler only on the first execution of the chart component\n  if (!myobj.resizeHandlerAttached){\n\n    // Ensure render is only triggered after resize events have stopped\n    var debouncedResize = _.debounce(function(){\n\n      // Show chart again.\n      myself.placeholder().children().css('visibility','visible');\n\n      // Change chart width\n      myself.chartDefinition.width = myself.placeholder().width();\n      myself.render( myself.query.lastResults() );\n    }, 200);\n\n    // Attach resize handler\n    $(window).resize(function(){\n\n      // Only trigger resize if the container has changed width\n      if ( myself.chartDefinition.width != myself.placeholder().width()){\n\n        // Temporarily hide chart so that overflow does not happen\n        myself.placeholder().children().css('visibility','hidden');\n\n        // Trigger the resize with debounce\n        debouncedResize();\n      }    \n    });\n\n    myobj.resizeHandlerAttached = true;\n  }\n  \n  }\n ","description":"Resource code","tooltip":"Resource code to include","order":19,"classType":""},{"name":"resourceType","type":"Label","value":"Javascript","description":"Type","tooltip":"Resource Type to include","order":19,"classType":""}],"type":"LayoutResourceCode","typeDesc":"Resource"},{"id":"9dea3039-4a7b-4a0d-f6ee-d28d06e99816","parent":"UnIqEiD","properties":[{"name":"name","type":"Id","value":"pie1_brand_js","description":"Name","tooltip":"Name","order":5,"classType":""},{"name":"resourceCode","type":"Resource","value":"/**设置第一个图形的提示框显示样式*/\r\n/**鼠标停留在图形上面的显示样式*/\r\n/**\r\n * 创建一个全局变量myPie,用于定制自己的柱状图。\r\n*/\r\nvar myPie1= {}; \r\n/**\r\n *  格式一：数字添加千分位符函数\r\n*/\r\n/*\r\nmyPie1.addCommas = function (nStr){\r\n    nStr += '';\r\n    x = nStr.split('.');\r\n    x1 = x[0];\r\n    x2 = x.length > 1 ? '.' + x[1] : '';\r\n    var rgx = /(\\d+)(\\d{3})/;\r\n    while (rgx.test(x1)){\r\n        x1 = x1.replace(rgx, '$1' + ',' + '$2');\r\n    }\r\n    x2 = x2.slice(0,3);\r\n    return x1 + x2;\r\n}; \r\n*/\r\n/**\r\n *  格式二：数字添加千分位符函数\r\n*/\r\nmyPie1.addCommas =  function (num){\r\n    // 判断 是否 是 数字\r\n    /*\r\n    if(typeof num !==\"number\"){\r\n        return false;\r\n    }\r\n    */\r\n    num = num.toString();   // 字符 化\r\n    var s = num.match(/e\\+(\\d+)$/),ext=0;;\r\n    num = num.replace(/e.+$/,'');\r\n    if( s ){\r\n        ext=Number(s[1]);\r\n    }\r\n    //分割小数点两边\r\n    var tA=num.split('.');\r\n    //有小数点则分割开来处理(小数点后面可能还跟有科学记数法表示)\r\n    if( tA.length>=2&&ext ){\r\n        if( tA.length>ext ){\r\n            tA[0] += tA[1].slice(0,ext-1);\r\n            tA[1] = tA[1].slice(ext-1,tA[1].length-1);\r\n        }else{\r\n            tA[0] += (tA[1]+\"0\".repeat(ext-tA.length));\r\n            tA[1]='';\r\n        }\r\n    }\r\n    tA[0] = tA[0].split('');//拆字符\r\n    for( var i=tA[0].length;(i-=3)>0; ){\r\n        tA[0].splice(i,0,',');\r\n    }\r\n    return tA[0].join('')+(tA[1]?'.'+tA[1]:'');//连起来\r\n}\r\n/**\r\n * 定制自己的饼图\r\n*/\r\nmyPie1.defaultPieSettings = function (){\r\n    var cde = this.chartDefinition;\r\n    \r\n    //general\r\n    //cde.width = 550;\r\n    cde.height = 280;\r\n    \r\n    // Data source\r\n    cde.crosstabMode = true;\r\n    \r\n    // Panels\r\n    //cde.title = \" 品牌可点击\",\r\n    //cde.titleSize = {width: '100%'},\r\n    cde.titlePaddings = '4%',\r\n    cde.titleFont = 'lighter 20px \"Open Sans\"',\r\n    cde.title_fillStyle = '#FFFFFF',\r\n    cde.titleLabel_textStyle = '#333333',\r\n    \r\n    \r\n    // Main Plot\r\n    cde.valuesOptimizeLegibility = true;\r\n    cde.valueVisible = true;\r\n    cde.valueFont = 'lighter 5px \"Open Sans\"';\r\n    cde.explodedSliceIndex = 1;\r\n    cde.explodedSliceRadius = '2%'; //饼图部分分离\r\n    cde.slice_strokeStyle = 'white';\r\n    cde.slice_offsetRadius = function(scene) {\r\n        return scene.isSelected() ? '5%' : '5%';\r\n        //alert(scene.isSelected());\r\n    },\r\n    \r\n    //// Color axes\r\n    cde.colors = [\r\n         /*\r\n        '#F4A20E', '#8EC21F', '#E70012'\r\n        */\r\n        '#F39C12','#00A65A','#DD4B39'\r\n    ];\r\n    \r\n    //Panels\r\n    cde.legend = true;\r\n    cde.legendShape = 'circle';\r\n    //cde.legendSize = {\r\n     //   width : '100%'\r\n    //};\r\n    cde.legendArea_fillStyle = '#FFFFFF';\r\n    cde.legendFont = 'normal 12px \"Open Sans\"';\r\n    cde.valuesFont = \"lighter 12px Open Sans\";\r\n    cde.valuesAnchor = 'center';\r\n    //cde.valuesLabelStyle = 'inside';\r\n    cde.valuesLabelStyle = 'linker';\r\n    /*\r\n    cde.valuesMask = {\r\n        value.percent\r\n        {category}={value}\\x0a({value.percent})\r\n    };\r\n    */\r\n    cde.valuesOverflow = 'Show';\r\n    cde.valuesVisible = true;\r\n    \r\n    // Chart/Interaction\r\n    cde.animate = true;\r\n    cde.selectable = true;\r\n    //cde.hoverable = true;\r\n    \r\n    //tooltips\r\n    cde.tooltipDelayIn = 20;\r\n    cde.tooltipDelayOut = 20;\r\n    cde.tooltipFade = true;    // 注意 设为true 的 情况\r\n    cde.tooltipGravity = 's';\r\n    cde.tooltipOpacity = 1;\r\n    cde.tooltipFollowMouse = true;\r\n    cde.tooltipEnabled = true; //提示框是否显示\r\n    //提示框格式设置\r\n    cde.tooltipFormat = function f(){\r\n        var series = this.getSeriesLabel(),\r\n            category = this.getCategory(),\r\n            value = myPie1.addCommas(this.getValue().toFixed(2));\r\n            //value = this.getValue();\r\n        return \"<div class='tooltipContainer'>\" +\r\n            \"<div class='tooltipTitle'>\" + category + \"</div>\" +  \r\n            //\"<div class='tooltipLabel'>\" + series + \"</div>\" +  \r\n            \"<div class='tooltipValue'>￥\" + value + \"</div>\" +\r\n        \"</div>\";\r\n    };\r\n}\r\n ","description":"Resource code","tooltip":"Resource code to include","order":19,"classType":""},{"name":"resourceType","type":"Label","value":"Javascript","description":"Type","tooltip":"Resource Type to include","order":19,"classType":""}],"type":"LayoutResourceCode","typeDesc":"Resource"},{"id":"4a53d53d-3a20-2b97-b992-3f955759e553","parent":"UnIqEiD","properties":[{"name":"name","type":"Id","value":"pie2_area_js","description":"Name","tooltip":"Name","order":5,"classType":""},{"name":"resourceCode","type":"Resource","value":" /**\r\n * 创建一个全局变量myPie,用于定制自己的柱状图。\r\n*/\r\nvar myPie2 = {}; \r\n/**\r\n * \r\n*/\r\nmyPie2.addCommas = function (nStr){\r\n    nStr += '';\r\n    x = nStr.split('.');\r\n    x1 = x[0];\r\n    x2 = x.length > 1 ? '.' + x[1] : '';\r\n    var rgx = /(\\d+)(\\d{3})/;\r\n    while (rgx.test(x1)){\r\n        x1 = x1.replace(rgx, '$1' + ',' + '$2');\r\n    }\r\n    x2 = x2.slice(0,3);\r\n    return x1 + x2;\r\n}; \r\n/**\r\n * 定制自己的饼图\r\n*/\r\nmyPie2.defaultPieSettings = function (){\r\n    var cde = this.chartDefinition;\r\n    \r\n    //general\r\n    //cde.width = 550;\r\n    cde.height = 280;\r\n    \r\n    // Data source\r\n    cde.crosstabMode = true;\r\n    \r\n    // Panels\r\n    //cde.title = \"区域可点击\",\r\n    //cde.titleSize = {width: '100%'},\r\n    cde.titlePaddings = '4%',\r\n    cde.titleFont = 'lighter 20px \"Open Sans\"',\r\n    cde.title_fillStyle = '#FFFFFF',\r\n    cde.titleLabel_textStyle = '#333333',\r\n    \r\n     // Main Plot\r\n    cde.valuesOptimizeLegibility = true;\r\n    cde.valueVisible = true;\r\n    cde.valueFont = 'lighter 5px \"Open Sans\"';\r\n    //cde.explodedSliceIndex = 1;\r\n    //cde.explodedSliceRadius = '2%'; //饼图部分分离\r\n    cde.slice_strokeStyle = 'white';\r\n    cde.slice_offsetRadius = function(scene) {\r\n        return scene.isSelected() ? '5%' : '5%';\r\n    },\r\n    \r\n    \r\n    //// Color axes\r\n    cde.colors = [\r\n        /*\r\n        '#F4A20E', '#8EC21F', '#E70012'\r\n        */\r\n        '#F39C12','#00A65A','#DD4B39'\r\n    ];\r\n\r\n    //Panels\r\n    cde.legend = true;\r\n    cde.legendShape = 'circle';\r\n    //cde.legendSize = {\r\n     //   width : '100%'\r\n    //};\r\n    cde.legendArea_fillStyle = '#FFFFFF';\r\n    cde.legendFont = 'normal 11px \"Open Sans\"';\r\n    cde.valuesFont = \"lighter 12px Open Sans\";\r\n    //cde.valuesAnchor = 'center';\r\n    //cde.valuesLabelStyle = 'inside';\r\n    cde.valuesLabelStyle = 'linker';\r\n    /*\r\n    cde.valuesMask = {\r\n        value.percent\r\n    };\r\n    */\r\n    cde.valuesOverflow = 'Show';\r\n    cde.valuesVisible = true;\r\n    \r\n    // Chart/Interaction\r\n    cde.animate = true;\r\n    cde.selectable = true;\r\n    cde.hoverable = true;\r\n    \r\n    //tooltips\r\n    cde.tooltipDelayIn = 20;\r\n    cde.tooltipDelayOut = 20;\r\n    cde.tooltipFade = true;\r\n    cde.tooltipGravity = 's';\r\n    cde.tooltipOpacity = 1;\r\n    cde.tooltipFollowMouse = true;\r\n    cde.tooltipEnabled = true; //提示框是否显示\r\n    //提示框格式设置\r\n    cde.tooltipFormat = function f(){\r\n        var series = this.getSeriesLabel(),\r\n            category = this.getCategory(),\r\n            value = myPie2.addCommas(this.getValue().toFixed(2));\r\n        return \"<div class='tooltipContainer'>\" +\r\n            \"<div class='tooltipTitle'>\" + category + \"</div>\"